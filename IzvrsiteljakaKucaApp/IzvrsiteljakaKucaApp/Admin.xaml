<Window x:Class="IzvrsiteljakaKucaApp.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IzvrsiteljakaKucaApp"
        mc:Ignorable="d"
        Closing="Window_Closing"
        Title="Admin" Height="450" Width="500">
    <Grid>
        <ListView x:Name="lista" HorizontalAlignment="Left" Height="248" Margin="10,10,0,0" VerticalAlignment="Top" Width="472">
            <ListView.View>
                <GridView x:Name="Grid">
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <TextBox x:Name="txtSQL" HorizontalAlignment="Left" Height="120" Margin="10,263,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="472" RenderTransformOrigin="0.529,0.323"/>
        <Button x:Name="btnIzvrsi" Content="Izvrsi" HorizontalAlignment="Left" Margin="35,389,0,0" VerticalAlignment="Top" Width="75" Click="btnIzvrsi_Click"/>
        <Button x:Name="bntNazad" Content="Nazad" HorizontalAlignment="Left" Margin="365,389,0,0" VerticalAlignment="Top" Width="75" Click="bntNazad_Click"/>

    </Grid>
</Window>
